def buildno = BUILD_NUMBER
pipeline {
    agent any
    tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "maven3.6.3"
    }
	
    stages {
      	
       stage('Build'){
             steps{
		    sh "mvn clean package"
			sh "whoami"
	              }
	      
          } 
	
	   
	  stage('Deploy'){
             steps{
		    ansiblePlaybook credentialsId: 'ansible credentials', inventory: 'hosts.inv', playbook: 'Deploy.yml'
              }
        }
	  
     }

}
